local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Envixity Hub V1.4",
   LoadingTitle = "Loading Tabs/Scripts/GUIS",
   LoadingSubtitle = "by yeeeter30",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = true,
      Invite = "YvMdVDRtgH", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Input key",
      Subtitle = "Key System",
      Note = "Key is Key & TestKey",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Key","TestKey"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

local Tab = Window:CreateTab("Player/Main", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Main")

local Label = Tab:CreateLabel("Player Username: " .. game.Players.LocalPlayer.Name)


local Label = Tab:CreateLabel("Player ID: " .. game.Players.LocalPlayer.UserId)

local gameId = game.PlaceId

-- Get the Game Name
local gameInfo = game:GetService("MarketplaceService"):GetProductInfo(gameId)
local gameName = gameInfo.Name

local Label = Tab:CreateLabel("Game ID: " .. gameId)
local Label = Tab:CreateLabel("Game Name: " .. gameName)

local Section = Tab:CreateSection("Player")

local Input = Tab:CreateInput({
   Name = "WalkSpeed",
   PlaceholderText = "Input Number",
   RemoveTextAfterFocusLost = false,
   Callback = function(WS)
   game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = WS
   end,
})

local Input = Tab:CreateInput({
   Name = "JumpPower",
   PlaceholderText = "Input Number",
   RemoveTextAfterFocusLost = false,
   Callback = function(JP)
   game.Players.LocalPlayer.Character.Humanoid.JumpPower = JP
   end,
})

local Input = Tab:CreateInput({
   Name = "Gravity",
   PlaceholderText = "Input Number",
   RemoveTextAfterFocusLost = false,
   Callback = function(GR)
    game.workspace.Gravity = GR
   end,
})

